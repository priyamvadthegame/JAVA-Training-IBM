class ComplexNumberTest
{
	public static void main(String args[])
	{
		Complex c=new Complex(2,3);
		Complex c1=new Complex(4,5);
		Complex add=c.add(c1);
		Complex substract=c.substract(c1);
		Complex multiply=c.multiply(c1);
		System.out.println("Addition ="+add.getReal()+"i"+add.getImaginary());
		System.out.println("Substraction ="+substract.getReal()+"i"+substract.getImaginary());
		System.out.println("Multiply ="+multiply.getReal()+"i"+multiply.getImaginary());
	}
}

		